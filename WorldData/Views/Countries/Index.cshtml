@{
  Layout = "_Layout";
}
<div class="container">
  <h1>Here are the list of countries</h1>
<div class="row">
  <div class="col-md-6">
    <h4>Country</h4>
<ul>
  @foreach(var country in @Model)
  {
    <li>@country._countryName </li>

  }
</ul>
  </div>
<div class="col-md-6">
  <h4>Country-Code</h4>
<ul>
  @foreach(var country in @Model)
  {
    
    <li>@country._countryCode </li>
  }
</ul>
</div>

</div>
</div>

